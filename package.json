{
  "name": "calebe-levelup-api",
  "version": "1.0.0",
  "description": "api for calebe project",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migration": "npx prisma migrate dev --name init --schema ./src/infra/database/prisma/schema.prisma",
    "migration:prod": "npx prisma migrate deploy --schema ./src/infra/database/prisma/schema.prisma",
    "migration:generate": "prisma generate --schema ./src/infra/database/prisma/schema.prisma",
    "test": "jest"
  },
  "prisma": {
    "schema": "src/infra/database/prisma/schema.prisma"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eduardaricaldes/calebe-levelup-api.git"
  },
  "keywords": [
    "calebe",
    "levelup"
  ],
  "author": "duuda2011@gmai.com,nossilesilva@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eduardaricaldes/calebe-levelup-api/issues"
  },
  "homepage": "https://github.com/eduardaricaldes/calebe-levelup-api#readme",
  "dependencies": {
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.4",
    "argon2": "^0.44.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.28.8",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.11",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "prisma-kysely": "^2.2.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
